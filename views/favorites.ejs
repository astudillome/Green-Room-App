<h2>Favorites</h2>

<% for (let i = 0; i < favorites.length; i++) {
    <p> <%= favorite[i].album_title %> </p>
    <form method="GET" action="/favorites/<%=favorites[i].album_title%>">
        <input hidden type="text" name="name" value="<%= favorites[i].masterID %>">
        <button class="btn btn-primary" type="submit">See Details</button>
<% } %>