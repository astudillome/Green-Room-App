<!-- Sign up form -->
<form action="/auth/signup" method="POST">
  <div class="form-group">
    <label for="new-email">Email address</label>
    <input type="email" class="form-control" id="new-email" aria-describedby="emailHelp">
    <small id="emailHelp" class="form-text text-muted">We'll never share your email with anyone else.</small>
  </div>

  <div class="form-group">
    <label for="new-firstName">First Name</label>
    <input type="text" class="form-control" id="new-firstName" aria-describedby="emailHelp">
  </div>

  <div class="form-group">
    <label for="new-lastName">Last Name</label>
    <input type="text" class="form-control" id="new-lastName" aria-describedby="emailHelp">
  </div>

  <div class="form-group">
    <label for="exampleInputPassword1">Password</label>
    <input type="password" class="form-control" id="exampleInputPassword1">
  </div>

  <button type="submit" class="btn btn-primary">Sign Up</button>
</form>